{
  "name": "reptiles",
  "description": "Generate svg images with beautiful and colorful tiles. Useful for creating attractive and pseudorandom, cacheable placeholders.",
  "version": "0.0.1",
  "private": true,
  "author": "Tim Mikeladze <tim.mikeladze@gmail.com>",
  "license": "MIT",
  "keywords": [
    "svg",
    "placeholder",
    "image placeholder",
    "tiles",
    "svg tiles",
    "color grid",
    "reptiles",
    "tiled"
  ],
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn codegen:watch\"",
    "codegen": "graphql-codegen --config codegen.yaml && git add generated",
    "codegen:watch": "yarn codegen && graphql-codegen --watch --config codegen.yaml",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@nextui-org/react": "1.0.0-beta.12",
    "@svgdotjs/svg.js": "3.1.2",
    "@types/randomcolor": "0.5.7",
    "apollo-server-micro": "3.12.0",
    "entropy-string": "4.2.0",
    "ga-gtag": "1.1.7",
    "graphql": "16.6.0",
    "graphql-middleware": "6.0.9",
    "graphql-modules": "2.1.1",
    "graphql-rate-limit": "3.3.0",
    "graphql-scalars": "1.20.4",
    "graphql-shield": "7.6.5",
    "ioredis": "5.3.1",
    "kofi-button": "1.1.1",
    "lodash": "4.17.21",
    "micro": "9.4.1",
    "nanoid": "4.0.1",
    "next": "12.3.4",
    "next-seo": "5.15.0",
    "next-themes": "0.2.1",
    "object-hash": "3.0.0",
    "randomcolor": "0.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svgdom": "0.1.13",
    "svgo": "2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/typescript-resolvers": "2.7.13",
    "@iconify/react": "3.2.2",
    "@iconify/types": "1.1.0",
    "@ryansonshine/commitizen": "4.2.8",
    "@ryansonshine/cz-conventional-changelog": "3.3.4",
    "@types/graphql": "14.5.0",
    "@types/ioredis": "4.28.10",
    "@types/lodash": "4.14.191",
    "@types/node": "17.0.43",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/svgo": "2.6.4",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "eslint-config-next": "12.3.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
