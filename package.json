{
  "name": "reptiles",
  "description": "Generate svg images with beautiful and colorful tiles. Useful for creating attractive and pseudorandom, cacheable placeholders.",
  "version": "0.0.1",
  "private": true,
  "author": "Tim Mikeladze <tim.mikeladze@gmail.com>",
  "license": "MIT",
  "keywords": [
    "svg",
    "placeholder",
    "image placeholder",
    "tiles",
    "svg tiles",
    "color grid",
    "reptiles",
    "tiled"
  ],
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn codegen:watch\"",
    "codegen": "graphql-codegen --config codegen.yaml && git add generated",
    "codegen:watch": "yarn codegen && graphql-codegen --watch --config codegen.yaml",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@nextui-org/react": "1.0.0-beta.6",
    "@svgdotjs/svg.js": "3.1.2",
    "@types/randomcolor": "0.5.6",
    "apollo-server-micro": "3.6.7",
    "entropy-string": "4.2.0",
    "ga-gtag": "1.1.1",
    "graphql": "16.4.0",
    "graphql-middleware": "6.0.9",
    "graphql-modules": "2.0.0",
    "graphql-rate-limit": "3.3.0",
    "graphql-scalars": "1.17.0",
    "graphql-shield": "7.5.0",
    "ioredis": "5.0.4",
    "kofi-button": "1.1.1",
    "lodash": "4.17.21",
    "micro": "9.3.4",
    "nanoid": "3.3.3",
    "next": "12.1.5",
    "next-seo": "5.4.0",
    "next-themes": "0.1.1",
    "object-hash": "3.0.0",
    "randomcolor": "0.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "svgdom": "0.1.10",
    "svgo": "2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript-resolvers": "2.6.1",
    "@iconify/react": "3.2.1",
    "@iconify/types": "1.1.0",
    "@types/graphql": "14.5.0",
    "@types/ioredis": "4.28.10",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.25",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/svgo": "2.6.3",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "commitizen": "4.2.4",
    "concurrently": "7.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
